<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{% endblock %}</title>
    <link rel="icon" href="{{ asset('build/images/' ~ site_favicon) }}" />
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body>
{% block body %}
    {% include 'layout/components/navbar.html.twig' %}
    <section class="uk-section uk-padding uk-padding-remove-horizontal">
        <div class="uk-container uk-margin-large" style="min-height: 340px;">
            {% block container %}{% endblock %}
        </div>
    </section>
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</body>
</html>
